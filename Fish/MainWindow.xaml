<Window x:Class="Fish.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fish"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <MediaElement x:Name="mediaElement1" HorizontalAlignment="Left" Height="419" VerticalAlignment="Top" Width="792" Loaded="manual" Unloaded="manual"/>
        <Button x:Name="play" Content="▶️ " HorizontalAlignment="Left" Margin="383,374,0,0" VerticalAlignment="Top" Width="28" Height="26" Click="play_Click" Unloaded="manual" Loaded="manual"/>
        <Button x:Name="pause" Content="⏸ " HorizontalAlignment="Left" Margin="334,375,0,0" VerticalAlignment="Top" Width="28" Height="25" Click="pause_Click" Loaded="manual" Unloaded="manual"/>
        <Button x:Name="stop" Content="⏹ " HorizontalAlignment="Left" Margin="292,375,0,0" VerticalAlignment="Top" Width="24" Height="25" Click="stop_Click" Unloaded="manual" Loaded="manual"/>
        <Button x:Name="add" Content="+" HorizontalAlignment="Left" Margin="747,375,0,0" VerticalAlignment="Top" Width="26" Click="add_Click" Height="25"/>
        <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" Margin="10,374,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
